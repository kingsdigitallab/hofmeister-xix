    <body>
        <group id="output">
            <groupHead>
                <groupTitle>Output parameters</groupTitle>
                <groupInfo />
            </groupHead>
            <item id="outputroot">
                <param>c:\user_output\hofmeister_xmod\01_output</param>
                <notes>
                    <p><?xm-replace_text {p}?></p>
                </notes>
                <internal>
                    <p />
                </internal>
            </item>
            <item id="outswitch">
                <param />
                <notes>
                    <p><?xm-replace_text {p}?></p>
                </notes>
                <internal>
                    <p />
                </internal>
            </item>
        </group>
        <group id="link">
            <groupHead>
                <groupTitle>Linking</groupTitle>
                <groupInfo><?xm-replace_text {groupInfo}?></groupInfo>
            </groupHead>
            <item id="linkroot">
                <param>file:///c:/user_output/hofmeister_xmod/01_output</param>
                <notes>
                    <p>file:///c:/user_output/hofmeister_xmod/01_output/</p>
                    <p>http://maple.cc.kcl.ac.uk/ps/hofmeister_xmod/web/</p>
                </notes>
                <internal>
                    <p />
